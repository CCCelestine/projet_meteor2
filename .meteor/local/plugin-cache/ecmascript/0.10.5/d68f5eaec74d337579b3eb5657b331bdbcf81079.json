rue,"sourceMapTarget":"client/config/FormHooks/Menus.js.map","sourceFileName":"client/config/FormHooks/Menus.js"}},"code":"AutoForm.hooks({\n  'ajoutMenu': {\n    onSubmit: function (doc) {\n      // Gestion du formulaire d'inscription\n      console.log(doc);\n      var error = null;\n      var commerces = doc.commerces;\n      var name = doc.name;\n      var description = doc.description;\n      var prix = doc.prix;\n      var rest = doc.rest;\n      var photo = doc.photo;\n      Menus.insert({\n        commerces: commerces,\n        name: name,\n        description: description,\n        prix: prix,\n        rest: rest,\n        photo: photo\n      }, function (err) {\n        if (err) {\n          error = new Error(\"Une erreur s'est produite\");\n        }\n      });\n\n      if (error === null) {\n        this.done(); // Appelle onSuccess\n      } else {\n        this.done(error); // Appelle onError\n      }\n\n      return false; // Dans tout les cas, arrete la soumission des donneés.\n    },\n    onSuccess: function () {\n      Router.go(Utils.pathFor('accueil'));\n    },\n    onError: function (formType, err) {\n      alert(err.reason);\n    }\n  }\n});","map":{"version":3,"sources":["client/config/FormHooks/Menus.js"],"names":["AutoForm","hooks","onSubmit","doc","console","log","error","commerces","name","description","prix","rest","photo","Menus","insert","err","Error","done","onSuccess","Router","go","Utils","pathFor","onError","formType","alert","reason"],"mappings":"AAAAA,SAASC,KAAT,CAAe;AACX,eAAa;AACTC,cAAU,UAASC,GAAT,EAAa;AAAE;AACrBC,cAAQC,GAAR,CAAYF,GAAZ;AACA,UAAIG,QAAQ,IAAZ;AACA,UAAIC,YAAaJ,IAAII,SAArB;AACA,UAAIC,OAAOL,IAAIK,IAAf;AACA,UAAIC,cAAcN,IAAIM,WAAtB;AACA,UAAIC,OAAOP,IAAIO,IAAf;AACA,UAAIC,OAAOR,IAAIQ,IAAf;AACA,UAAIC,QAAQT,IAAIS,KAAhB;AAEAC,YAAMC,MAAN,CAAa;AACTP,mBAAWA,SADF;AAETC,cAAMA,IAFG;AAGTC,qBAAaA,WAHJ;AAITC,cAAMA,IAJG;AAKTC,cAAMA,IALG;AAMTC,eAAOA;AANE,OAAb,EAOG,UAASG,GAAT,EAAa;AACZ,YAAGA,GAAH,EAAO;AACHT,kBAAQ,IAAIU,KAAJ,CAAU,2BAAV,CAA